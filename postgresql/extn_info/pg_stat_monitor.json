      "guc",
      "pgsm_query_id",
      "functions",
      "counters",
      "relations",
      "database",
      "error_insert",
      "application_name",
      "application_name_unique",
      "top_query",
      "cmd_type",
      "error",
      "rows",
      "tags",
      "user"
    ]
  }
}